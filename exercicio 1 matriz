matriz = [[0] * 4 for _ in range(4)]

for i in range(4):
    for j in range(4):
        matriz[i][j] = int(input(f"Digite o valor para a posição [{i}][{j}]: "))

contador = 0
for linha in matriz:
    for valor in linha:
        if valor > 10:
            contador += 1

print(f"A matriz possui {contador} valores maiores que 10.")
