valores = []
for i in range(5):
    valor = float(input("Digite um valor: "))
    valores.append(valor)


print("Valores lidos:")
for valor in valores:
    print(valor)


maior = max(valores)
menor = min(valores)
media = sum(valores) / len(valores)


print("Maior valor: ", maior)
print("Menor valor: ", menor)
print("MÃ©dia dos valores: ", media)
